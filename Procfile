operator: bin/darwin/kubernikus operator --auth-username=$OS_USERNAME --auth-domain=$OS_USER_DOMAIN_NAME --auth-password=$OS_PASSWORD --auth-project=$OS_PROJECT_NAME --auth-project-domain=$OS_PROJECT_DOMAIN_NAME --auth-url=$OS_AUTH_URL --namespace=$NAMESPACE --context=$KUBECONTEXT --v=5
api: bin/darwin/apiserver --context=$KUBECONTEXT --namespace=$NAMESPACE --auth-url=$OS_AUTH_URL --v=5
